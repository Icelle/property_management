<%= simple_form_for @building do |f| %>
  <%= f.input :street %>
  <%= f.input :city %>
  <%= f.input :state, as: :select, collection: controller.class::US_STATES %>
  <%= f.input :postal_code %>
  <%= f.input :description, as: :text %>
  <%= f.association :owners, collection: Owner.print_owners, as: :select, include_blank: true %>
  <%= f.button :submit %>
<% end %>
